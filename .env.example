# Copia este archivo como .env y ajusta valores seguros
CONFIG_SERVER_USER=config
CONFIG_SERVER_PASSWORD=config
CONFIG_GIT_URI=https://github.com/JoshNisth/eventos-config-repo.git
ENCRYPT_KEY=CHANGE_ME_GENERATE_A_SECURE_RANDOM_HEX
KEYCLOAK_ADMIN_CLIENT_SECRET=CHANGE_ME_KEYCLOAK_CLIENT_SECRET
SSL_KEYSTORE_PASSWORD=

# Keycloak SMTP (opcional)
# Si KEYCLOAK_SMTP_HOST se deja vac√≠o, no se configura SMTP.
KEYCLOAK_SMTP_HOST=
KEYCLOAK_SMTP_PORT=587
KEYCLOAK_SMTP_FROM=
KEYCLOAK_SMTP_FROM_DISPLAY_NAME=Eventos
KEYCLOAK_SMTP_USER=
KEYCLOAK_SMTP_PASSWORD=
KEYCLOAK_SMTP_STARTTLS=true
KEYCLOAK_SMTP_SSL=false
KEYCLOAK_SMTP_AUTH=true
